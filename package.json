{
  "name": "platzi-posts",
  "version": "1.0.0",
  "description": "Proyecto demo para implementar Cypress",
  "author": "edsadr",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open --project ./test",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "build:test": "cross-env NODE_ENV=test node build/build.js",
    "test:server": "pushstate-server dist",
    "test:init": "npm run test:server",
    "test:dev": "npm run build:test && run-p --race test:server cypress:open",
    "test:clean": "cd build && node clean-test-db.js"
  },
  "dependencies": {
    "firebase": "^4.13.1",
    "moment": "^2.22.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.0",
    "cypress": "^3.1.2",
    "dotenv-webpack": "^1.5.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "firebase-admin": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "node-notifier": "^5.3.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.19",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.2.1",
    "pushstate-server": "^3.0.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "semver": "^5.6.0",
    "shelljs": "^0.7.6",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.7.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
